{
	"extends": "./.svelte-kit/tsconfig.json",
  "paths": {
    "@constants": [
      "src/constants"
    ],
		"@constants/*": ["src/constants/*"],
    "@types": [
      "src/types"
    ],
		"@types/*": ["src/types/*"]
	},
	"compilerOptions": {
		"allowJs": true,
		"checkJs": true,
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"resolveJsonModule": true,
		"skipLibCheck": true,
		"sourceMap": true,
		"strict": true
	}
}
